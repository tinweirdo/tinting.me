    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "fast-xml-parser": "^4.1.2",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "i": "^0.3.7",
    "less": "^4.1.3",
    "magic-string": "^0.30.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
